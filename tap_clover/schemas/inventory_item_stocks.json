{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Inventory Item Stocks",
  "type": "object",
  "properties": {
    "item_id": {
      "type": "string"
    },
    "merchant_id": {
      "type": "string"
    },
    "stockCount": {
      "type": "integer"
    },
    "quantity": {
      "type": "number"
    },
    "modifiedTime": {
      "type": "integer"
    }
  },
  "required": [
    "item_id",
    "merchant_id"
  ]
}
