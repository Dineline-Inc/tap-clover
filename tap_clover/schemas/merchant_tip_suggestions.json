{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Merchant Tip Suggestions",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "merchant_id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "percentage": {
      "type": "integer"
    },
    "amount": {
      "type": "integer"
    },
    "isEnabled": {
      "type": "boolean"
    },
    "flatTip": {
      "type": "integer"
    }
  },
  "required": [
    "id",
    "merchant_id"
  ]
}
