{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Merchant Default Service Charges",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "merchant_id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "enabled": {
      "type": "boolean"
    },
    "percentage": {
      "type": "integer"
    },
    "percentageDecimal": {
      "type": "integer"
    },
    "isAutoApplied": {
      "type": "boolean"
    }
  },
  "required": [
    "id",
    "merchant_id"
  ]
}
