{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Merchant Tenders",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "merchant_id": {
      "type": "string"
    },
    "editable": {
      "type": "boolean"
    },
    "labelKey": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "opensCashDrawer": {
      "type": "boolean"
    },
    "supportsTipping": {
      "type": "boolean"
    },
    "enabled": {
      "type": "boolean"
    },
    "visible": {
      "type": "boolean"
    },
    "instructions": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "merchant_id"
  ]
}
